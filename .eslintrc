{
  "extends": "airbnb-base",
  "globals": {
    "describe": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "before": true,
    "afterEach": true,
    "after": true
  },
  "rules": {
    "no-console": "off",
    "no-unused-expressions": "off",
    "no-underscore-dangle": [
      2,
      {
        "allow": [
          "_id",
          "__v"
        ]
      }
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  }
}